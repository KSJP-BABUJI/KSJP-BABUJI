/* Write a program to separate even numbers one side and odd numbers one side in given array */
#include <stdio.h>
int main()
{
  int a[100],n,i;
  printf("Enter array size ");
  scanf("%d",&n); 
  printf("Enter %d numbers\n",n);
  for(i=0;i<n;i++)
  scanf("%d",&a[i]);
  printf("Output:- ");
  for(i=0;i<n;i++)
    if(a[i]%2==0)
      printf("%d  ",a[i]) ;
  for(i=0;i<n;i++)
    if(a[i]%2==1)
      printf("%d  ",a[i]) ;
  return 0; 
}
